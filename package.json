    "test": "jest --no-cache"
  },
  "types": "dist/module.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
